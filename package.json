{
  "name": "gatsby-starter-minimal-ts",
  "version": "1.0.0",
  "private": true,
  "description": "A Gatsby starter written in TypeScript with no plugins and a single welcome page.",
  "author": "Jude Agboola",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "typecheck": "tsc --noEmit"
  },
  "license": "0BSD",
  "dependencies": {
    "@deckdeckgo/highlight-code": "^3.6.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "gatsby": "^5.12.4",
    "gatsby-plugin-react-helmet": "^6.12.0",
    "gatsby-plugin-sitemap": "^6.12.3",
    "gatsby-remark-highlight-code": "^3.3.0",
    "gatsby-remark-katex": "^7.12.0",
    "gatsby-source-filesystem": "^5.12.1",
    "gatsby-transformer-remark": "^6.12.3",
    "katex": "^0.13.24",
    "re-resizable": "^6.9.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.35",
    "@types/react-dom": "^18.2.14",
